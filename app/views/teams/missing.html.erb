<% @missing.each do |registration| %>
  <% user = registration.user %>
  <%= form_tag(registrations_path) do %>
  <br /><%= user.name %> / <%= user.email %>: 
  <%= hidden_field_tag 'membership[game_id]', @game.id %>
  <%= hidden_field_tag 'membership[user_id]', user.id %>
  <%= collection_select(:registration, :team_id, @game.teams, :id, :name) %>
  <%= submit_tag 'Add to Team' %>
  <% end %>
<% end %>