<% @missing.each do |registration| %>
  <% user = registration.user %>
  <%= form_tag(registrations_path) do %>
  <br /><%= user.name %> / <%= user.email %>: 
  <%= hidden_field_tag 'registration[game_id]', @game.id %>
  <%= hidden_field_tag 'registration[user_id]', user.id %>
  <%= hidden_field_tag 'registration[agree_waiver]', true %>
  <%= hidden_field_tag 'registration[agree_photo]', true %>
  <%= hidden_field_tag 'registration[legal_name]', 'Admin-approved Email Submitter' %>

  <%= collection_select(:registration, :team_id, @game.teams, :id, :name) %>
  <%= submit_tag 'Add to Team' %>
  <% end %>
<% end %>